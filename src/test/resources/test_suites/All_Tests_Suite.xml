<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="4">

    <test name="Run CareerBuilder Search">
    	<parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.job.JobSearchMaven.tests.CareerBuilderSearchTest" />
      </classes>
    </test>
    
    <test name="Run Dice Search">
    	<parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.job.JobSearchMaven.tests.DiceSearchTest" />
      </classes>
    </test>
    
    <test name="Run LinkedIn Search">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.job.JobSearchMaven.tests.LinkedInSearchTest" />
      </classes>
    </test>
    
    <test name="Run GlassDoor In Search">
    	<parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.job.JobSearchMaven.tests.GlassDoorSearchTest" />
      </classes>
    </test>
    
</suite>